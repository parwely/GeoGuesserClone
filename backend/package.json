{
  "name": "geoguessr-clone-backend",
  "version": "1.0.0",
  "description": "Backend API for GeoGuessr Clone Android App",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "db:migrate": "node src/database/migrate.js up",
    "db:rollback": "node src/database/migrate.js down",
    "db:seed": "node src/database/seeders/locations.js",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "build": "echo 'No build step required for Node.js'",
    "vercel": "vercel",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch --detectOpenHandles"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "testTimeout": 30000
  },
  "keywords": [
    "geoguessr",
    "game",
    "backend",
    "api",
    "android"
  ],
  "author": "Mira Speier",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@types/pg": "^8.15.4",
    "@vercel/node": "^2.3.0",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "node-cache": "^5.1.2",
    "pg": "^8.16.3",
    "pg-format": "^1.0.4",
    "postgis": "^1.0.5",
    "redis": "^4.6.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.15",
    "jest": "^30.0.4",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.3"
  }
}
